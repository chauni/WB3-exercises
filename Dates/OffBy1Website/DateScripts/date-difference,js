// gather inputs
let startDate = Date.parse("July 11, 2022");
let endDate = Date.parse("November 11, 2022");

//perform calculation

let msec_per_day = 1000 * 60 * 60 * 24
let elapsedMilliseconds = endDate - startDate
let dayDiff = elapsedMilliseconds / msec_per_day
let numDays = Math.round(dayDiff)


// write output
console.log(`The number of days between dates is ${numDays}`);
